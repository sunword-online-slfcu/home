var isMobileSupportPage = null!==document.getElementById("mobile_support_page");if(isMobileSupportPage){var gliaCoBrowsingLink=document.createElement("a");function showVisitorCode(){var o=document.createElement("sm-visitor-code");document.body.appendChild(o)}gliaCoBrowsingLink.className="button omnibrowse-code-button",gliaCoBrowsingLink.setAttribute("data-role","button"),gliaCoBrowsingLink.innerText=window.Alkami.Localization.SiteText["Glia.Mobile.CoBrowse"]?window.Alkami.Localization.SiteText["Glia.Mobile.CoBrowse"]:"Show CoBrowsing Code",document.querySelector("div.modal-content")&&document.querySelector("div.modal-content").append(gliaCoBrowsingLink),document.querySelector(".omnibrowse-code-button")&&document.querySelector(".omnibrowse-code-button").addEventListener("click",showVisitorCode)};